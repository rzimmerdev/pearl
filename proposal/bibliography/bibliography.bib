  isbn                       = {0-87590-427-0},
  issn                       = {0065-8448},
  keywords-plus              = {BROMINATED FLAME RETARDANTS; POROUS-MEDIA; CONTAMINANT TRANSPORT; ORGANIC CONTAMINANTS; HYDRAULIC-PROPERTIES; SOLUTE TRANSPORT; DEGRADATION; WATER; AQUIFER; MATRIX},
  language                   = {English},
  number-of-cited-references = {58},
