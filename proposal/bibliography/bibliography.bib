  eissn                      = {1432-1122},
  journal-iso                = {Financ. Stoch.},
  keywords                   = {Limit order book; Market microstructure noise; Micro-price; Realized variance; Sampling schemes},
  keywords-plus              = {MARKET MICROSTRUCTURE NOISE; HIGH-FREQUENCY DATA; INTEGRATED VOLATILITY; FINANCIAL-MARKETS; TIME; FORECASTS; EXCHANGE; SAMPLE},
  language                   = {English},
