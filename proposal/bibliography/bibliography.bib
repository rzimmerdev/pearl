@Article{Katoch2021,
  author   = {Sourabh Katoch and Sumit Singh Chauhan and Vijay Kumar},
  journal  = {Multimedia Tools and Applications},
  title    = {A review on genetic algorithm: past, present, and future},
  year     = {2021},
  issn     = {1573-7721},
