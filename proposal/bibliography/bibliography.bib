@InProceedings{Cont2022,
  author                     = {Cont, Rama and Cucuringu, Mihai and Kochems, Jonathan A. and Prenzel, Felix},
  booktitle                  = {3RD ACM INTERNATIONAL CONFERENCE ON AI IN FINANCE, ICAIF 2022},
  title                      = {Dynamic Calibration of Order Flow Models with Generative Adversarial Networks},
  year                       = {2022},
  address                    = {1601 Broadway, 10th Floor, NEW YORK, NY, UNITED STATES},
  note                       = {3rd ACM International Conference on AI in Finance (ICAIF), New York, NY, NOV 02-04, 2022},
  organization               = {Assoc Comp Machinery; J P Morgan Chase \& Co; U S Bank},
  pages                      = {446-453},
  publisher                  = {ASSOC COMPUTING MACHINERY},
  abstract                   = {Classical models for order flow dynamics based on point processes, such
   as Poisson or Hawkes processes, have been studied intensively. Often,
   several days of limit border book (LOB) data is used to calibrate such
   models, thereby averaging over different dynamics such as intraday
   effects or different trading volumes.
   This work uses generative adversarial networks (GANs) to learn the
   distribution of calibrations - obtained by many calibrations based on
   short time frames. The trained GAN can then be used to generate
